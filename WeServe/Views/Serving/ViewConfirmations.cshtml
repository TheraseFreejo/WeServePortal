@model IEnumerable<WeServe.Models.BookingModel>
@{
    ViewBag.Title = "ViewConfirmations";
    Layout = "~/Views/Shared/Serve_InnerLayout.cshtml";
}

<h2>ViewConfirmations</h2>
<table border="1">
    <thead class="thead-default">
        <tr>

            <th data-priority="3">BOOKING ID</th>
            <th data-priority="3">DATE</th>
            <th data-priority="2">DESCRIPTION</th>
            @*<th data-priority="2">userid</th>*@
            <th data-priority="3">USERNAME</th>
            @*<th data-priority="2">service id</th>*@
            <th data-priority="2">SERVICE</th>
         
        </tr>
    </thead>
    <tbody>
        @foreach (var o in Model)
        {
            <tr>
                <td>@o.bookid</td>
                <td>@o.bookdate</td>
                <td>@o.description</td>
                @*<td>@o.userid</td>*@
                <td>@o.username</td>
                @*<td>@o.serviceid</td>*@
                <td>@o.service</td>
                @*<td>@Html.ActionLink("Confirm Booking", "Confirm", new { id = o.bookid }, null)</td>*@
            </tr>
        }

    </tbody>
</table>